<%= form_for(@signup, url: invitation_repeal_path, html: { method: :delete}) do |f| %>
  <div><%= hidden_field_tag 'signup[event_id]', @event.id %></div>
  <div><%= hidden_field_tag 'signup[guest_id]', session[:user_id] %></div>
  <%= f.submit "Repeal Request", class: "btn btn-primary" %>
<% end %>
